@model SisDec.Models.Cliente
@{
    ViewBag.Title = "DetalhesClienteJuridica";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-horizontal">
    <hr />

    <div class="control-group">
        @Html.LabelFor(model => model.Nome, new { @class = "control-label" })
        <div class="controls">
            @Html.DisplayFor(model => model.Nome, new { @class = "form-control" })
        </div>
    </div>
    <div class="control-group">
        @Html.LabelFor(model => model.Cnpj, new { @class = "control-label" })
        <div class="controls">
            @Html.DisplayFor(model => model.Cnpj, new { @class = "form-control" })
        </div>
    </div>
    <div class="control-group">
        @Html.LabelFor(model => model.objCidade, new { @class = "control-label col-md-2" })
        <div class="controls">
            @Html.DisplayFor(model => model.objCidade.Nome, new { @class = "form-control" })
        </div>
    </div>
</div>

<p>
    @Html.ActionLink("Voltar", "Listar", null, new { @class = "btn btnlg btn-success" })
</p>