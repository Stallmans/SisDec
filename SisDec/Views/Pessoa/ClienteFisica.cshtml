@model SisDec.Models.PessoaFisica



@{
    ViewBag.Title = "Inserir Novo Cliente Pessoa Fisica";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="main">
        <div class="main-inner">
            <div class="container">
                <div class="row">
                    <div class="span12">
                        <div class="widget widget-nopad">
                            <div class="widget-header">
                                <i class="icon-list-alt"></i>
                                <h3>Cadastrar Cliente - Física</h3>
                            </div>
                            <!-- /widget-header -->
                            <div class="widget-content">
                                <div class="widget big-stats-container">
                                    <div class="widget-content">
                                        <div class="form-horizontal">
                                            <hr />

                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Nome, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Nome, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Cpf, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Cpf, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.DataNascimento, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.DataNascimento, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Cep, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Cep, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Complemento, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Complemento, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Email, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Email, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Endereco, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Endereco, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Numero, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Numero, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Rendimento, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Rendimento, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Sexo, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Sexo, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="control-group">
                                                @Html.LabelFor(model => model.Telefone, new { @class = "control-label" })
                                                <div class="controls">
                                                    @Html.TextBoxFor(model => model.Telefone, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="form-actions">
                                                <button type="submit" class="btn btn-primary">Salvar</button>
                                                <button class="btn">Cancelar</button>
                                            </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /widget-content -->

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
}
